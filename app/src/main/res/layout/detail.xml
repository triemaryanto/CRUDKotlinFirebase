                <TextView
                    android:id="@+id/tdata6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"></TextView>

                <TextView
                    android:id="@+id/tdata7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"></TextView>

                <TextView
                    android:id="@+id/tdata8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"></TextView>

                <TextView
                    android:id="@+id/tdata9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"></TextView>
                <TextView
                    android:id="@+id/tdata10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"></TextView>
                <TextView
                    android:id="@+id/tdata11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"></TextView>
                <TextView
                    android:id="@+id/tdata12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"></TextView>
                <TextView
                    android:id="@+id/tdata13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"></TextView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="11. Kerugian Sementara"
                    android:textStyle="bold"></TextView>

                <TextView
                    android:id="@+id/tkerugiansementara"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"></TextView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="12. Diketahui Kacab"
                    android:textStyle="bold"></TextView>

                <TextView
                    android:id="@+id/tdiketahui"
